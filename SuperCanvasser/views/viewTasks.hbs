<h1>{{subtitle}}</h1>
<br>
<br>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Username</th>
            <th scope="col">Date</th>
            <th scope="col">Current Location</th>
            <th scope="col">Edit</th>
        </tr>
    </thead>
    <tbody>
    <div id="tasks">
        {{#each tasks}}
            <tr>
                <div id="{{taskID}}" class="campaign">
                    <td><p style="font-size: 16px;">{{username}}</p></td>
                    <td><p style="font-size: 16px;">{{date}}</p></td>
                    <td><p style="font-size: 16px;">{{currentLocation}}</p></td>
                    <td><p style="font-size: 16px;"><a href="/viewTask/{{taskID}}">Details</a></p></td>
                </div>
            </tr>
        {{/each}}
    </div>
    </tbody>
</table>
